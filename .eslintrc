{
  "extends": "oclif",
  "rules": {
    "array-bracket-spacing": [
      2,
      "always"
    ],
    "arrow-parens": [
      0
    ],
    "indent": [
      2,
      2,
      {
        "MemberExpression": 1
      }
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "object-curly-spacing": [
      2,
      "always"
    ]
  }
}
